<VirtualHost *:80>
    ServerName app1.washingtonk21.local
    DocumentRoot /var/www/vhosts/app1/Public
    ErrorLog /var/log/httpd/app1_log/error.log
    CustomLog /var/log/httpd/app1_log/access.log combined
</VirtualHost>

